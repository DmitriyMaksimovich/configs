[core]
	excludesFile = /home/dmitriy/.gitignore
[user]
	email = 1dimakss@gmail.com
	name = Dmitriy Maksimovich
[alias]
	lg = log --graph --pretty=format:'%C(yellow)%d%Creset %C(cyan)%h%Creset %s %Cgreen(%cr)' --abbrev-commit --date=short --all
	lg2 = log --graph --pretty=format:'%C(yellow)%d%Creset %C(cyan)%h%Creset %s %Cgreen(%cr)' --abbrev-commit --date=short --all
	co = checkout
	br = branch
	cmt = commit
	st = status
	unstage = reset HEAD --
[format]
	pretty = format:%C(auto)%h %d%Creset %s%n%Cgreen%ad%Creset %aN <%aE>%n
